programa {
  funcao inicio() {
    
    inteiro num1, num2

    escreva("Maior ou menor \n")

    escreva("Digite um número: ")
    leia(num1)

    escreva("Digite outro número: ")
    leia(num2)

    se(num1 > num2){
      escreva("O numero ", num1, " é o maior")
    } senao {
      escreva("O numero ", num2, " é o maior")
    }



  }
}
